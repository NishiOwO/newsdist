#ifndef __MACROS__
#define __MACROS__

.Dt NAME SECTION
.Dd DATE
.Sh "NAME"
NAME_LOWER \- DESCRIPTION
.Sh "DESCRIPTION"

#define AUTHOR_HERE	Original was written by \fB\&AUTHOR\&\fP.
#define START_LIST	.Bl -tag -width
#define END_LIST	.El
#define FILE		.It Pa
#define ITEM		.It Fl

#endif
